#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules/gh-pages/bin/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules/gh-pages/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules/gh-pages/bin/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules/gh-pages/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/gh-pages@6.3.0/node_modules:/home/runner/work/sku/sku/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
else
  exec node  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
fi
