import { describe, it } from 'vitest';
import { runTest } from '../../../utils/test-utils.js';

const tests = [
  {
    fixtureName: '{{ codemodName }}',
  },
];

describe('[CODEMOD]: "{{ codemodName }}"', () => {
  it.for(tests)('"$fixtureName"', async ({ fixtureName }) => {
    await runTest({
      dirName: __dirname,
      transformName: '{{ codemodName }}',
      fixtureName,
      testOptions: {
        extension: 'tsx',
      }
    });
  });
});
