{
  "name": "sku",
  "version": "0.0.0-development",
  "description": "Front-end development toolkit, powered by Webpack, Babel, CSS Modules, Less and Jest",
  "main": "index.js",
  "bin": {
    "sku": "./bin/sku.js"
  },
  "scripts": {
    "test": "npm run format-check && jest test/index",
    "format": "prettier --single-quote --write '{bin,config,scripts}/**/*.js'",
    "format-check": "prettier --single-quote --list-different '{bin,config,scripts}/**/*.js'",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "lint-staged": {
    "{bin,config,scripts}/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/sku.git"
  },
  "keywords": [
    "toolkit",
    "webpack"
  ],
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/sku/issues"
  },
  "homepage": "https://github.com/seek-oss/sku#readme",
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^10.0.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "bluebird": "^3.5.1",
    "chalk": "^2.1.0",
    "command-line-args": "^4.0.7",
    "cross-spawn": "^5.1.0",
    "css-in-js-loader": "^0.1.2",
    "css-loader": "^0.28.7",
    "deasync-promise": "^1.0.1",
    "dir-compare": "^1.4.0",
    "eslint-config-sku": "^1.2.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "fs-extra": "^4.0.2",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^4.0.0",
    "jest": "^22.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "opn": "^5.1.0",
    "postcss-js": "^1.0.1",
    "postcss-loader": "^2.0.7",
    "prettier": "~1.9.0",
    "raw-loader": "^0.5.1",
    "start-server-webpack-plugin": "^2.2.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.19.0",
    "svgo": "^1.0.0",
    "svgo-loader": "^2.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "semantic-release": "^8.0.3",
    "validate-commit-msg": "^2.14.0"
  }
}
