{
  "name": "sku",
  "version": "0.0.0-development",
  "description": "Front-end development toolkit, powered by Webpack, Babel, CSS Modules, Less and Jest",
  "main": "index.js",
  "bin": {
    "sku": "./bin/sku.js"
  },
  "scripts": {
    "test": "npm run format-check && OPEN_TAB=false jest test/test-cases/*/*.test.js",
    "test-manual": "node test/manual",
    "format": "prettier --single-quote --write '{bin,config,scripts}/**/*.js'",
    "format-check": "prettier --single-quote --list-different '{bin,config,scripts}/**/*.js'",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "commitlint --edit --extends seek",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "lint-staged": {
    "{bin,config,scripts}/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/sku.git"
  },
  "keywords": [
    "toolkit",
    "webpack"
  ],
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/sku/issues"
  },
  "homepage": "https://github.com/seek-oss/sku#readme",
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-flow-react-proptypes": "^10.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-seek-style-guide": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "bluebird": "^3.5.1",
    "chalk": "^2.4.1",
    "command-line-args": "^4.0.7",
    "commander": "^2.16.0",
    "cross-spawn": "^6.0.5",
    "css-in-js-loader": "^0.1.2",
    "css-loader": "^0.28.7",
    "deasync-promise": "^1.0.1",
    "dedent": "^0.7.0",
    "empty-dir": "^1.0.0",
    "eslint": "^4.13.1",
    "eslint-config-seek": "^3.1.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "find-up": "^3.0.0",
    "fs-extra": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^6.0.0",
    "jest": "^23.4.2",
    "kopy": "^8.3.0",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "opn": "^5.3.0",
    "postcss-js": "^1.0.1",
    "postcss-loader": "^2.1.6",
    "prettier": "~1.9.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.1.3",
    "start-server-webpack-plugin": "^2.2.5",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.21.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "url-loader": "^1.0.1",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^4.16.4",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.1",
    "async-disk-cache": "^1.3.3",
    "child-process-promise": "^2.2.1",
    "commitizen": "^2.9.6",
    "commitlint-config-seek": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "death": "^1.1.0",
    "es6-promisify": "^6.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "node-dir": "^0.1.17",
    "node-fetch": "^2.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "semantic-release": "^8.0.3",
    "wait-on": "^2.1.0"
  }
}
